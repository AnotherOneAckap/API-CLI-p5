#!/usr/bin/env perl
use strict;
use warnings;
use 5.010;
use Data::Dumper;
use FindBin '$Bin';
use lib "$Bin/../../lib";

package API::CLI::MetaCPAN;
use base 'API::CLI';

sub add_auth {
}

package main;
use YAML::XS qw/ LoadFile /;

my $openapi_file = "$Bin/../metacpan-openapi.yaml";
my $openapi = LoadFile($openapi_file);
my $cli = API::CLI::MetaCPAN->new({
    openapi => $openapi,
    name => "metacpancl",
});
$cli->run;
